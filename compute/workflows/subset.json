{
        "name": "SUBSET",
        "author": "CMCC",
        "abstract": "workflow which implements a subset using oph_subset",
        "exec_mode": "sync",
        "ncores": "1",
	"on_exit": "oph_delete",
        "tasks":
        [
                {
                        "name": "ImportNC",
                        "operator": "oph_importnc",
                        "arguments": [ "measure=$1", "src_path=$2", "subset_dims=$5", "subset_filter=$6", "subset_type=$7" ]
                },
                {
                        "name": "ExportNC2",
                        "operator": "oph_exportnc2",
                        "arguments": [ "output_path=$3", "output_name=$4" ],
                        "dependencies": [  { "task": "ImportNC", "type": "single" } ]
                }
                
        ]
}
